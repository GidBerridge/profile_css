,{"generated":{"line":76,"column":24},"source":"javascript/slider.js","original":{"line":86,"column":26}},{"generated":{"line":77,"column":4},"source":"javascript/slider.js","original":{"line":87,"column":4}},{"generated":{"line":78,"column":4},"source":"javascript/slider.js","original":{"line":87,"column":5}},{"generated":{"line":79,"column":2},"source":"javascript/slider.js","original":{"line":88,"column":2}},{"generated":{"line":79,"column":3},"source":"javascript/slider.js","original":{"line":88,"column":3}},{"generated":{"line":79,"column":4},"source":"javascript/slider.js","original":{"line":88,"column":4}},{"generated":{"line":80,"column":0},"source":"javascript/slider.js","original":{"line":89,"column":0}},{"generated":{"line":80,"column":1},"source":"javascript/slider.js","original":{"line":89,"column":1}},{"generated":{"line":81,"column":0},"source":"javascript/slider.js","original":{"line":89,"column":2}}],"sources":{"javascript/slider.js":"export const slider = function(){\n\n  const slides = document.querySelectorAll('.slide');\n  const btnLeft = document.querySelector('.slider__btn--left');\n  const btnRight = document.querySelector('.slider__btn--right');\n  const dotContainer = document.querySelector('.dots');\n\n\n  let curSlide = 0;\n  const maxSlide = slides.length;\n\n  // Functions\n\n  const createDots =function(){\n    slides.forEach(function(_, i) {\n      dotContainer.insertAdjacentHTML('beforeend', \n      `<button class=\"dots__dot\" data-slide=\"${i}\"></button>`);\n    })\n  }\n\n\n  const activateDot = function(slide) {\n    document.querySelectorAll('.dots__dot')\n    .forEach(dot => dot.classList.remove('dots__dot--active'));\n\n    document.querySelector(`.dots__dot[data-slide=\"${slide}\"]`)\n    .classList.add('dots__dot--active');\n  };\n\n\n\n  const goToSlide = function(slide) {\n      slides.forEach(\n      (s, i) => (s.style.transform = `translateX(${100 * (i - slide)}%)`)\n      );\n  }\n\n\n\n  // Next slide\n  const nextSlide = function() {\n      if (curSlide === maxSlide - 1){\n      curSlide = 0;\n    } else {\n    curSlide++;\n    };\n\n    goToSlide(curSlide);\n    activateDot(curSlide);\n  };\n\n  const prevSlide = function() {\n    if (curSlide === 0){\n    curSlide = maxSlide -1;\n    } else {\n    curSlide--;\n    }\n    goToSlide(curSlide);\n    activateDot(curSlide);\n    };\n\n\n  // init function\n    const init = function() {\n      goToSlide(0);\n      createDots();\n      activateDot(0);\n    }\n\n    init();\n\n    // Event handlers \n  btnRight.addEventListener('click', nextSlide);\n  btnLeft.addEventListener('click', prevSlide);\n\n  document.addEventListener('keydown', function(e) {\n    // console.log(e);\n    if(e.key === 'ArrowLeft') prevSlide();\n    e.key === 'ArrowRight' && nextSlide(); // Short circuited version\n  });\n\n  dotContainer.addEventListener('click', function (e) {\n      if (e.target.classList.contains('dots__dot')) {\n        const { slide } = e.target.dataset;\n        goToSlide(slide);\n        activateDot(slide);\n    };\n  });\n};\n"},"lineCount":null}},"error":null,"hash":"fb7200893bb06379064365643856605a","cacheData":{"env":{}}}